<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lgy.login.ssologin.mapper.UserMapper">
    <select id="findUserByNameAndPasaWorld" parameterType="String" resultType="Map">
        select id,name,passworld from sys_users where name =#{name} and passworld = #{passWorld}
    </select>
</mapper>